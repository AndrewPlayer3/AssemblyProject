lnk programs/function

.function addInts
    add r0, r1
    mov r11, r0
    ret

.function subInts
    sub r0, r1
    mov r11, r0
    ret

.function doAddThenSub
    run addInts
    run subInts
    ret

.main 
    mov r3, ' \0
    mov r0, 10
    mov r1, 15
    run addInts
    out r11
    pln r3
    run subInts
    out r11
    pln r3
    mov r0, This is a string.\0
    run printString
