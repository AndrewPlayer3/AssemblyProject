#Width Question
mov r10, Width?\0
pln r10

#Box Width
ini r0

#Height Question
mov r10, Height?\0
pln r10

#Box Height
ini r10

#Box Type Question
mov r13, Filled (1) or Hollow (2)?\0
pln r13

#Box Type
ini r12

#New Line
mov r13, \0
pln r13

#Test Value
mov r13, 1

#Row Count
mov r1, 1

row_loop:
    #Column Count
    mov r2, 1

    column_loop:
        #goto tests
        jmp testing

        #Print an x
        print_x:
        mov r5, x\0
        put r5
        
        jmp end_loop

        print_space:
        mov r5, ' '\0
        put r5

        end_loop:
        #col++
        add r2, 1
        
        #Col > Width?
        cmp r2, r0
        jle column_loop

    #New Line
    mov r5, \0
    pln r5

    #row++
    add r1, 1

    #Row > Height
    cmp r1, r10
    jle row_loop

#Goto finish once out of loop
jmp finish

testing:
    cmp r13, r12
    je print_x
    cmp r2, r13
    je print_x
    cmp r1, r13
    je print_x
    cmp r2, r0
    je print_x
    cmp r1, r10
    je print_x
    jmp print_space

finish:
    #This is the end




































