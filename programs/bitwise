question:
    mov r0, Push or Pop?\0
    pln r0
    ins r0
    mov r1, Push\0
    mov r2, Pop\0
    mov r3, Loop\0
    cms r0, r1
    je do_push
    cms r0, r2
    je do_pop
    cms r0, r3
    je do_loop
    jmp question

do_push:
    mov r0, Enter a number to push:\0
    pln r0
    ini r0
    psh r0
    jmp question

do_pop:
    pop r4
    mov r0, Popped \0
    put r0
    out r4
    mov r0, ' into R4.\0
    pln r0
    jmp question

do_loop:
    mov r0, 69
    psh r0
    jmp do_loop

